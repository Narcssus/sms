<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
    <classPathEntry location="C:\Users\Administrator\.m2\repository\mysql\mysql-connector-java\5.1.46\mysql-connector-java-5.1.46.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <plugin type="com.narc.MyPluginAdapter">
            <property name="baseDir" value="D:/Narc/IdeaProjects/sms"/>
            <property name="targetProject" value="src/main/java"/>
            <property name="targetProjectXml" value="src/main/resources"/>
            <property name="targetPackage" value="com.narc.sms.service.sms.dao.mapper"/>
            <property name="targetPackageService" value="com.narc.sms.service.sms.dao.service"/>
            <property name="targetPackageXml" value="mapping"/>
            <property name="isOverWrite" value="false"/>
        </plugin>
        <commentGenerator type="com.narc.MyCommentGenerator">
            <property name="author" value="Narc"/>
        </commentGenerator>
        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://cdb-353dbplv.gz.tencentcdb.com:10150/smsdb?serverTimezone=UTC"
                        userId="root"
                        password="Narc0912">
            <property name="useInformationSchema" value="true" />
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="true"/>
        </javaTypeResolver>
        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="com.narc.sms.service.sms.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapping" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.narc.sms.service.sms.dao.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->
        <table tableName="TXT_SMS_LOG" domainObjectName="TxtSmsLog"/>
        <table tableName="TXT_SMS_TASK" domainObjectName="TxtSmsTask"/>
        <table tableName="TXT_SMS_TEMPLATE" domainObjectName="TxtSmsTemplate"/>
        <table tableName="TXT_SMS_WHITE_PHONE" domainObjectName="TxtSmsWhitePhone"/>
        <table tableName="TXT_THREAD_COMPETITION_INFO" domainObjectName="TxtThreadCompetitionInfo"/>



    </context>
</generatorConfiguration>